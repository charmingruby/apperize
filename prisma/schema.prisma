datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Account {
  id String @id @default(uuid())

  @@map("users")
}

model PersonalRequest {
  id     String  @id @default(uuid())
  userId String?
  cpf    String
  reason String

  @@map("personal_requests")
}
