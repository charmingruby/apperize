generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Account {
  id String @id @default(uuid())

  @@map("users")
}

model CustomerProfile {
  id String @id @default(uuid())

  @@map("customers")
}

model EnterpriseProfile {
  id String @id @default(uuid())

  @@map("enterprises")
}

model Request {
  id String @id @default(uuid())

  @@map("requests")
}
